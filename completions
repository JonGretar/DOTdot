source ~/.bash.d/completion_scripts/git_completion
source ~/.bash.d/completion_scripts/svn_completion
complete -C ~/.bash.d/completion_scripts/rake_completion -o default rake
complete -C ~/.bash.d/completion_scripts/project_completion -o default c
complete -o default -o nospace -F _git gh
complete -C $ERL_LIBS/nitrogen-master/support/nitrogen_bashcompletion.escript -o default nitrogen